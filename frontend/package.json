{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "quality:check-all": "npm run lint:check; npm run styles:check",
    "quality:fix-all": "npm run lint:fix; npm run styles:fix",
    "lint:check": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "styles:check": "prettier --check '**/*.{js,jsx,ts,tsx,json,md}'",
    "styles:fix": "prettier --write '**/*.{js,jsx,ts,tsx,json,md}'",
    "preview": "vite preview",
    "prepare": "cd .. && husky install frontend/.husky"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.0",
    "@aws-sdk/client-s3": "^3.386.0",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2",
    "react-slider": "^2.0.6",
    "sort-by": "^1.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-security": "^1.7.1",
    "prettier": "^3.0.3",
    "vite": "^4.4.5",
    "vite-plugin-mkcert": "^1.16.0"
  }
}
